{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "ChaTach will access your photos to set a profile picture."
        }
      ]
    ],
    "name": "chAttach",
    "slug": "chAttach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.expo.chatach",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expo.chatach"
    },
    "android": {
      "package": "com.expo.chatach",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "992ddef7-3c4b-40a8-8b70-851cc5b64c4c"
      }
    }
  }
}
